package com.emilyread.zookeeper;

public class ZooTest {

	public static void main(String[] args) {
		Gorilla g= new Gorilla();
		
		System.out.println("Initial energy: "+ g.displayEnergy());
		
		g.throwSomething();
		g.throwSomething();
		g.throwSomething();
		
		
		
		g.eatBanana();
		g.eatBanana();
		
		
		
		g.climb();
		
		
	}

}
